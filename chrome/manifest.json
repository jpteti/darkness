{
	"manifest_version": 2,
	"name": "Darkness",
	"version": "1.0",
	"description": "Improves The Washington Post with superior slogans.",
	"content_scripts": 
	[
		{
			"matches": ["*://www.washingtonpost.com/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}